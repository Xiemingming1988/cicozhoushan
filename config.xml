<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kingee.zhoushan" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>舟山阳光</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <preference name="iosPersistentFileLocation" value="Compatibility" />
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/icon/ios/i_icon.png" width="180" />
        <icon height="60" src="res/icon/ios/i_icon.png" width="60" />
        <icon height="120" src="res/icon/ios/i_icon.png" width="120" />
        <icon height="76" src="res/icon/ios/i_icon.png" width="76" />
        <icon height="152" src="res/icon/ios/i_icon.png" width="152" />
        <icon height="40" src="res/icon/ios/i_icon.png" width="40" />
        <icon height="80" src="res/icon/ios/i_icon.png" width="80" />
        <icon height="57" src="res/icon/ios/i_icon.png" width="57" />
        <icon height="114" src="res/icon/ios/i_icon.png" width="114" />
        <icon height="72" src="res/icon/ios/i_icon.png" width="72" />
        <icon height="144" src="res/icon/ios/i_icon.png" width="144" />
        <icon height="29" src="res/icon/ios/i_icon.png" width="29" />
        <icon height="58" src="res/icon/ios/i_icon.png" width="58" />
        <icon height="50" src="res/icon/ios/i_icon.png" width="50" />
        <icon height="100" src="res/icon/ios/i_icon.png" width="100" />
        <splash height="480" src="res/screen/ios/splash.png" width="320" />
        <splash height="960" src="res/screen/ios/splash.png" width="640" />
        <splash height="1024" src="res/screen/ios/splash.png" width="768" />
        <splash height="2048" src="res/screen/ios/splash.png" width="1536" />
        <splash height="768" src="res/screen/ios/splash.png" width="1024" />
        <splash height="1536" src="res/screen/ios/splash.png" width="2048" />
        <splash height="1136" src="res/screen/ios/splash.png" width="640" />
        <splash height="1334" src="res/screen/ios/splash.png" width="750" />
        <splash height="2208" src="res/screen/ios/splash.png" width="1242" />
        <splash height="1242" src="res/screen/ios/splash.png" width="2208" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-camera" spec="~4.0.3" />
    <plugin name="cordova-plugin-file" spec="~6.0.1" />
    <plugin name="cordova-plugin-file-opener2" spec="~2.0.19" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.7.1" />
    <plugin name="cordova-plugin-imagepicker" spec="~1.1.5" />
    <plugin name="cordova-plugin-media" spec="~5.0.2" />
    <plugin name="cordova-plugin-media-capture" spec="~3.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <engine name="ios" spec="~4.5.5" />
</widget>
